# Uncomment the next line to define a global platform for your project
platform :ios, ’10.0’

target 'fos' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!



  # Pods for fos

  pod ‘STTwitter’
  pod 'Alamofire', '~> 4.3'
  pod 'FacebookCore'
  pod 'FacebookLogin'
  pod 'FacebookShare'
  pod 'SDWebImage'

  target 'fosTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'fosUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
